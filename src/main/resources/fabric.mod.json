{
  "schemaVersion": 1,
  "id": "kinswathe",
  "version": "${version}",
  "name": "KinsWathe",
  "description": "Kin's Wathe is an addon mod for WATHE by doctor4t. And it also undergoes some balance modifications and fix some issues present in Noelle's Roles.",
  "authors": ["Bsxin"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/kinswathe/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.BsXinQin.kinswathe.client.KinsWatheDataGenerator"
    ],
    "client": [
      "org.BsXinQin.kinswathe.client.KinsWatheClient"
    ],
    "main": [
      "org.BsXinQin.kinswathe.KinsWathe"
    ],
    "cardinal-components": [
      "org.BsXinQin.kinswathe.KinsWatheComponents"
    ]
  },
  "custom": {
    "cardinal-components": [
      "kinswathe:ability",
      "kinswathe:config",
      "kinswathe:custom_winner",
      "kinswathe:cook"
    ]
  },
  "mixins": [
    "kinswathe.mixins.json",
    {
      "config": "kinswathe.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "harpymodloader": "*"
  }
}
